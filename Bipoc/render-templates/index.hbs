<div class="flex-column">
  <div id="outer-container-div-id" class="outer-container-div-class">
    <div id="inner-container-div-left-id" class="inner-container-div-left-class">
      <img id="img-foreground-inner-container-left-id" class="img-foreground-inner-container-left-class"
        src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSXypUxCYXfjJWZInGyiAKXH3z5IA-eKluQIw&usqp=CAU"
        alt="logo">
    </div>
    <div id="inner-container-div-right-id" class="inner-container-div-right-class">
      <div class="inner-grid-container-div-class">
        <img id="img-logo-inner-container-right-id" class="img-logo-inner-container-right-class"
          src="data:image/png;base64,"
          alt="cream city logo">
      </div>

      <div class="innermost-flex-container-div-class">

        <h4 id="innermost-container-header-h2-id" class="innermost-container-header-h2-class">Available positions.
          People hiring.</h4>
        <p id="innermost-container-p-id" class="innermost-container-p-class">Find tomorrow's candidate's today</p>

        <button tabindex="0" id="landing-login-btn" class="innermost-container-button-login-class">Log
          in</button>

        <button tabindex="0" id="landing-signup-btn" class="innermost-container-button-signup-class">Sign up</button>


      </div>
    </div>
    <!--inner right container-->
  </div>
  <!--end 1st section-->
  <div class="outter-container-middle-top-class">
    <div id="" class="inner-container-middle-top-left-div-class">
      <h3 class="inner-container-middle-top-left-h3-top-class">You're here to hire.</h3>
      <h3 class="inner-container-middle-top-left-h3-bottom-class">We are here to help.</h3>
      <p class="inner-container-middle-top-left-p-class"> Search for qualified candidates to fill your job openings,
        <br> make offers on ____. Start hiring today.
      </p>
    </div>
    <div id="" class="inner-container-middle-top-right-div-class">
      <p class="inner-container-middle-top-right-p-class">SUGGESTED SEARCHES</p>
      <div class="inner-container-middle-top-right-div-grid-class">
        <button class="inner-container-middle-top-right-button-class">Placeholder: 1</button>
        <button class="inner-container-middle-top-right-button-class">Placeholder: 2</button>
        <button class="inner-container-middle-top-right-button-class">Placeholder: 3</button>
        <button class="inner-container-middle-top-right-button-class">Placeholder: 4</button>
        <button class="inner-container-middle-top-right-button-class">Placeholder: 5</button>
        <button class="inner-container-middle-top-right-button-class">Placeholder: 6</button>
      </div>
    </div>
  </div>
  <!--end 2nd section-->
  <div class="outter-container-middle-inner-top-class">
    <div class="outter-container-middle-inner-top-left-class">
      <div class="circle"> <img id="creamCityLogoId" src="/bipoc-logo.png" alt="Cream city logo" height="275px"
          width="325px"> </div>
    </div>
    <!--end left column-->
    <div class="outter-container-middle-inner-top-right-class">
      <h3 class="outter-container-middle-inner-top-right-h3-class">Join your friends, family, colleagues on BIPOC</h3>
      <button class="outter-container-middle-inner-top-right-button-class">Join a forum discussion today.</button>
    </div>
  </div>
  <!--end 3rd section-->
  <div class="outter-container-middle-bottom-class">
    <button></button>
  </div>
</div>
<!-- end 4th section-->


<div class="login-modal-main" id="{{LOGIN_MODAL}}">
  <div class="login-modal-content">
    <span class="close"></span>
    <form id="{{LOGIN_FORM}}" class="modal-content animate" onsubmit="return false">
      <div class="container">
        <label>
          <for="username">Username
        </label>
        <input id="{{MEMBER_SIGNUP_USERNAME}}" type="text" name="username" placeholder="username" required>
        <label for="email"><b>Email</b></label>
        <input id="{{MEMBER_SIGNUP_EMAIL}}" type="email" placeholder="johnSmith@gmail.com" name="email" required>
        <label for="psw"><b>Password</b></label>
        <input id="{{MEMBER_SIGNUP_PASSWORD}}" type="password" placeholder="Enter Password" name="psw" required>
        <input id="{{LOGIN_OK}}" type="submit" value="LOGIN">

        <button type="button" id="{{LOGIN_CANCEL}}">Cancel</button>
        <span class="psw">Forgot <a href="#">password?</a></span>

        <div class="flash-message" id="{{FLASH_MESSAGE}}"></div>
      </div>
    </form>
  </div>
</div>


